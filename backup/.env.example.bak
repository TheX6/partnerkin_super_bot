# Файл примера переменных окружения
# Скопируйте этот файл в .env и заполните своими данными

# Токен Telegram бота (получите в @BotFather)
TELEGRAM_TOKEN=ваш_токен_здесь

# Параметры подключения к PostgreSQL
DB_TYPE=postgresql
DB_HOST=localhost
DB_PORT=5432
DB_NAME=partnerkino
DB_USER=postgres
DB_PASSWORD=ваш_пароль

# Параметры администратора (опционально)
ADMIN_PASSWORD=partnerkin1212

# Порт для веб-сервера (если планируете webhook)
PORT=3000

# Режим разработки
NODE_ENV=production